{
  "name": "lunaxcode.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:init": "tsx scripts/init-db.ts",
    "db:seed-pricing": "tsx scripts/seed-pricing.ts",
    "db:seed-addons": "tsx scripts/seed-addons.ts",
    "db:create-admin": "tsx scripts/create-admin-user.ts",
    "d1:create": "wrangler d1 create lunaxcode-cms",
    "d1:create-dev": "wrangler d1 create lunaxcode-cms-dev",
    "d1:migrate": "drizzle-kit migrate",
    "d1:migrate-dev": "wrangler d1 migrations apply lunaxcode-cms-dev --local",
    "d1:migrate-prod": "wrangler d1 migrations apply lunaxcode-cms --remote",
    "d1:generate": "drizzle-kit generate",
    "d1:studio": "drizzle-kit studio",
    "d1:shell": "wrangler d1 execute lunaxcode-cms --command=\".tables\"",
    "d1:shell-dev": "wrangler d1 execute lunaxcode-cms-dev --local --command=\".tables\"",
    "migrate:d1": "tsx scripts/migrate-to-d1.ts",
    "migrate:data": "tsx scripts/migrate-data-to-d1.ts",
    "migrate:data:dev": "tsx scripts/migrate-data-to-d1.ts --dev",
    "migrate:data:prod": "tsx scripts/migrate-data-to-d1.ts --prod",
    "migrate:data:all": "tsx scripts/migrate-data-to-d1.ts --dev --prod",
    "deploy:pages": "pnpm build && wrangler pages deploy",
    "deploy:setup": "tsx scripts/migrate-to-d1.ts && pnpm d1:migrate-prod",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:dev": "npx @cloudflare/next-on-pages --dev",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "cloudflare:enable-edge": "find src/app/api -name 'route.ts' -exec sed -i '' 's|\\/\\/ export const runtime = '\\''edge'\\''\\;|export const runtime = '\\''edge'\\''\\;|' {} \\;",
    "cloudflare:disable-edge": "find src/app/api -name 'route.ts' -exec sed -i '' 's|export const runtime = '\\''edge'\\''\\;|\\/\\/ export const runtime = '\\''edge'\\''\\;|' {} \\;"
  },
  "dependencies": {
    "@better-auth/utils": "^0.3.0",
